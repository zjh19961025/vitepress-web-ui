{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "build/description/components.d.ts",
    "build/description/auto-imports.d.ts",
    "unocss.theme.ts"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "allowJs": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "@hua5/hua5-utils/global",
      "@hua5/hua5-web-lib/global",
      "@hua5/hua5-web-ui/global",
      "@hua5/unocss-preset/global"
    ]
  }
}
