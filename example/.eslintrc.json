{
  "root": true,
  "extends": [
    "@zjh/eslint-config",
    "./build/description/.eslintrc-auto-import.json"
  ],
  "rules": {
    // 自定义规则
    // 使用别名报错关闭
    "import/no-unresolved": "off",
    // 不需要显示说明后缀
    "import/extensions": "off",
    "import/namespace": "off",
    // .native' modifier on 'v-on' directive is deprecated.
    "vue/no-deprecated-v-on-native-modifier": "off"
  }
}
